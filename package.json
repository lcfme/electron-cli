    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.10.0",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.3.2",
    "eslint-plugin-standard": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-chmod": "^2.0.0",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^3.0.1",
    "gulp-util": "^3.0.7",
    "mocha": "^3.1.0",
    "mock-fs": "^3.12.1",
    "nyc": "^9.0.1",
    "run-sequence": "^1.2.2",
    "stream-combiner2": "^1.1.1"
