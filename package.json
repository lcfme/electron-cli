{
  "name": "electron-cli",
  "version": "0.0.9",
  "description": "Command line tool for developing useful electron apps",
  "main": "index.js",
  "scripts": {
    "test": "gulp mocha",
    "start": "./dist/cli.js",
    "dev": "gulp dev",
    "coverage": "nyc --reporter=lcov --all --reporter=text-lcov npm test"
