    "chalk": "^1.1.3",
    "configstore": "^2.1.0",
    "cpy": "^4.0.1",
    "exit": "^0.1.2",
    "inquirer": "^1.2.1",
    "jsonfile": "^2.4.0",
    "leek": "0.0.23",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.6.3",
    "nopt": "^3.0.6",
    "ora": "^0.3.0",
    "resolve": "^1.1.7",
    "semver": "^5.3.0",
    "username": "^2.2.2",
    "validate-npm-package-name": "^2.2.2",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
